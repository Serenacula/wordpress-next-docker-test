{
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "pristas-peter/wp-graphql-gutenberg",
                "version": "0.3.11",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/pristas-peter/wp-graphql-gutenberg/archive/refs/tags/v0.3.11.zip"
                }
            }
        }
    ],
    "require": {
        "pristas-peter/wp-graphql-gutenberg": "0.3.11",
        "wpackagist-plugin/wp-graphql": "1.6.12"
    },
    "extra": {
        "installer-paths": {
            "/var/www/html/wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "/var/www/html/wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}
